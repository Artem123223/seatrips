mixin curd(twoCard)
    .col
        .card.twoCard
            .tCard
                img.card-img-top(src=twoCard.img)
                if twoCard.isBike
                    p.biker
                        img.superimg(src="img/biker.svg", alt="велосипед")
                else
                    |
            .card-body
                h5.card-title #{twoCard.name}
                p.card-text #{twoCard.description}
                .g-flex
                    p.prices #{twoCard.price}
                    .hover-a
                        a.btns(href='#') замовити
                            svg.absoluteimg.bi.bi-arrow-right-circle(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
