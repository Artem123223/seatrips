mixin card(oneCard)
    figure.boxcard.co-2
        figcaption.box-padding
            p.p-width.text-align
                img.imgbox(src=oneCard.img)
            if oneCard.isStar
                p.star
                    img.starimg(src="img/star.png", alt="")
            else
                p
            p.currency #{oneCard.name}
            p.price #{oneCard.price}
            p.description #{oneCard.description}

