mixin curd(twoCard)
    .col
        .card.twoCard(style='width: 18rem;')
            img.card-img-top(src=twoCard.img)
            .card-body
                h5.card-title #{twoCard.name}
                p.card-text #{twoCard.description}
                .g-flex
                    p.prices #{twoCard.price}
                    a.btns(href='#') замовити
                        img.absoluteimg(src="img/secondarrowright.png", alt="стрілка вправо")
                p.stars
                    img.starimgs(src="img/star.png", alt="")
                if twoCard.isBike
                    p.biker
                        img.superimg(src="img/biker.png", alt="велосипед")
                else
                    |